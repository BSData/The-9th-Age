        <characteristic name="Str" typeId="7d4e-b182-dd11-52a0">+1</characteristic>
        <characteristic name="AP" typeId="646b-1e72-1589-5083"/>
        <characteristic name="Attributes" typeId="048a-df92-bb5b-6de9">Hand Weapon. Attacks made with a Bastard Sword gain +1 Strength and Devastating Charge (+1 AP)</characteristic>
      </characteristics>
    </profile>
  </sharedProfiles>
