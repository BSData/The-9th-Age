    <rule id="a61a-53e4-b9bf-9179" name="Lethal Strike (Riders only)" hidden="false">
      <modifiers/>
    </rule>
    <rule id="2574-5d3f-0232-26be" name="Lightning Rage" hidden="false">
      <description>The model has a Ward Save (2+) against attacks with the Lightning Attack special rule. If the model is hit by a Lightning Attack it gains Frenzy.</description>
      <modifiers/>
    </rule>
