    <entry id="6017-4a83-cc3d-7912" name="Altar of the Undeath" points="400.0" categoryId="9a00-8903-5d68-e8f5" type="unit" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="2" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false">
