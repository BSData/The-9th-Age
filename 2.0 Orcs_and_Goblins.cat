    <profile id="eae8-4e22-5826-d83e" name="Flail" book="BRB" page="70" hidden="false" profileTypeId="576561706f6e23232344415441232323" profileTypeName="Weapon">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
